@article{Terribilini01072007,
author = {Terribilini, Michael and Sander, Jeffry D. and Lee, Jae-Hyung and Zaback, Peter and Jernigan, Robert L. and Honavar, Vasant and Dobbs, Drena}, 
title = {RNABindR: a server for analyzing and predicting RNA-binding sites in proteins}, 
volume = {35}, 
