@article{Khorshid01012011,
author = {Khorshid, Mohsen and Rodak, Christoph and Zavolan, Mihaela}, 
title = {CLIPZ: a database and analysis environment for experimentally determined binding sites of RNA-binding proteins}, 
volume = {39}, 
number = {suppl 1}, 
pages = {D245-D252}, 
year = {2011}, 
doi = {10.1093/nar/gkq940}, 
abstract ={The stability, localization and translation rate of mRNAs are regulated by a multitude of RNA-binding proteins (RBPs) that find their targets directly or with the help of guide RNAs. Among the experimental methods for mapping RBP binding sites, cross-linking and immunoprecipitation (CLIP) coupled with deep sequencing provides transcriptome-wide coverage as well as high resolution. However, partly due to their vast volume, the data that were so far generated in CLIP experiments have not been put in a form that enables fast and interactive exploration of binding sites. To address this need, we have developed the CLIPZ database and analysis environment. Binding site data for RBPs such as Argonaute 1-4, Insulin-like growth factor II mRNA-binding protein 1-3, TNRC6 proteins A-C, Pumilio 2, Quaking and Polypyrimidine tract binding protein can be visualized at the level of the genome and of individual transcripts. Individual users can upload their own sequence data sets while being able to limit the access to these data to specific users, and analyses of the public and private data sets can be performed interactively. CLIPZ, available at http://www.clipz.unibas.ch, aims to provide an open access repository of information for post-transcriptional regulatory elements.}, 
URL = {http://nar.oxfordjournals.org/content/39/suppl_1/D245.abstract}, 
eprint = {http://nar.oxfordjournals.org/content/39/suppl_1/D245.full.pdf+html}, 
journal = {Nucleic Acids Research} 
}

